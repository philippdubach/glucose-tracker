    "axes[1, 1].set_title('Data Quality Scores')\n",
    "axes[1, 1].set_ylabel('Score (%)')\n",
    "axes[1, 1].set_ylim(0, 100)\n",
    "\n",
    "# Add percentage labels\n",
    "for i, (key, value) in enumerate(quality_scores.items()):\n",
    "    axes[1, 1].text(i, value + 1, f'{value:.1f}%', ha='center')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "print(f\"\\n✅ Overall Data Quality Score: {np.mean(list(quality_scores.values())):.1f}%\")"
   ]
