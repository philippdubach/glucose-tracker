    "## 4. Glucose Pattern Analysis {#patterns}\n",
    "\n",
    "Let's analyze more advanced glucose patterns including variability and trends."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate advanced glucose metrics\n",
    "def calculate_glucose_variability(data):\n",
    "    \"\"\"Calculate glucose variability metrics\"\"\"\n",
    "    # Rate of change\n",
    "    roc = data.diff() / (data.index.to_series().diff().dt.seconds / 60)\n",
    "    \n",
    "    # Mean Absolute Glucose (MAG)\n",
    "    mag = np.abs(data.diff()).mean()\n",
    "    \n",
    "    # Continuous Overlapping Net Glycemic Action (CONGA)\n",
    "    # Using 1-hour intervals\n",
    "    hourly_data = data.resample('H').mean().dropna()\n",
    "    if len(hourly_data) > 1:\n",
    "        conga = hourly_data.diff().std()\n",
    "    else:\n",
    "        conga = np.nan\n",
    "    \n",
    "    return {\n",
    "        'roc_mean': roc.mean(),\n",
    "        'roc_std': roc.std(),\n",
    "        'mag': mag,\n",
    "        'conga': conga\n",
    "    }\n",
    "\n",
    "variability_metrics = calculate_glucose_variability(glucose_data)\n",
    "\n",
    "print(\"📈 GLUCOSE VARIABILITY ANALYSIS\")\n",
    "print(\"=\" * 40)\n",
    "print(f\"📊 Mean rate of change: {variability_metrics['roc_mean']:.3f} mmol/L/min\")\n",
    "print(f\"📏 Rate of change std: {variability_metrics['roc_std']:.3f} mmol/L/min\")\n",
    "print(f\"🔄 Mean Absolute Glucose (MAG): {variability_metrics['mag']:.2f} mmol/L\")\n",
    "print(f\"🌊 CONGA (1h): {variability_metrics['conga']:.2f} mmol/L\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Advanced pattern analysis visualization\n",
    "fig, axes = plt.subplots(2, 2, figsize=(16, 12))\n",
    "fig.suptitle('Advanced Glucose Pattern Analysis', fontsize=16, fontweight='bold')\n",
    "\n",
    "# 1. Rate of change analysis\n",
    "roc = glucose_data.diff() / (glucose_data.index.to_series().diff().dt.seconds / 60)\n",
    "roc_clean = roc[(roc > -2) & (roc < 2)]  # Remove extreme outliers\n",
    "\n",
    "axes[0, 0].hist(roc_clean, bins=50, alpha=0.7, color='purple', edgecolor='black')\n",
    "axes[0, 0].axvline(0, color='red', linestyle='--', label='Zero change')\n",
    "axes[0, 0].axvline(roc_clean.mean(), color='orange', linestyle='--', \n",
    "                   label=f'Mean: {roc_clean.mean():.3f}')\n",
    "axes[0, 0].set_title('Rate of Change Distribution')\n",
    "axes[0, 0].set_xlabel('Rate of Change (mmol/L/min)')\n",
    "axes[0, 0].set_ylabel('Frequency')\n",
    "axes[0, 0].legend()\n",
    "\n",
    "# 2. Glucose stability over time\n",
    "daily_cv = glucose_data.groupby(glucose_data.index.date).agg(\n",
    "    lambda x: x.std() / x.mean() * 100 if x.mean() != 0 else 0\n",
    ")\n",
    "\n",
    "axes[0, 1].plot(daily_cv.index, daily_cv.values, \n",
    "                marker='o', linewidth=2, color='green')\n",
    "axes[0, 1].axhline(daily_cv.mean(), color='red', linestyle='--', \n",
    "                   label=f'Average CV: {daily_cv.mean():.1f}%')\n",
    "axes[0, 1].set_title('Daily Glucose Variability (CV%)')\n",
    "axes[0, 1].set_xlabel('Date')\n",
    "axes[0, 1].set_ylabel('Coefficient of Variation (%)')\n",
    "axes[0, 1].legend()\n",
    "axes[0, 1].tick_params(axis='x', rotation=45)\n",
    "axes[0, 1].grid(True, alpha=0.3)\n",
    "\n",
    "# 3. Glucose heatmap by hour and day\n",
    "glucose_pivot = glucose_data.to_frame()\n",
    "glucose_pivot['hour'] = glucose_data.index.hour\n",
    "glucose_pivot['date'] = glucose_data.index.date\n",
    "pivot_table = glucose_pivot.pivot_table(\n",
    "    values='Historic Glucose mmol/L', \n",
    "    index='date', \n",
    "    columns='hour', \n",
    "    aggfunc='mean'\n",
    ")\n",
    "\n",
    "im = axes[1, 0].imshow(pivot_table.values, cmap='RdYlBu_r', aspect='auto')\n",
    "axes[1, 0].set_title('Glucose Heatmap (Date vs Hour)')\n",
