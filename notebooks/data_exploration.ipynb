    "# Create integrated timeline\n",
    "def create_activity_timeline(glucose_df, sleep_df, workout_df, nutrition_df):\n",
    "    \"\"\"Create timeline with all activities marked\"\"\"\n",
    "    \n",
    "    # Start with glucose data\n",
    "    timeline = glucose_df[['Historic Glucose mmol/L']].copy()\n",
    "    timeline['Glucose_interpolated'] = timeline['Historic Glucose mmol/L'].interpolate()\n",
    "    \n",
    "    # Add activity markers\n",
    "    timeline['Sleep'] = 0\n",
    "    timeline['Workout'] = 0\n",
    "    timeline['Nutrition'] = 0\n",
    "    \n",
    "    # Mark sleep periods\n",
    "    for _, sleep in sleep_df.iterrows():\n",
    "        mask = (timeline.index >= sleep['Start']) & (timeline.index <= sleep['End'])\n",
    "        timeline.loc[mask, 'Sleep'] = 1\n",
    "    \n",
    "    # Mark workout periods\n",
    "    for _, workout in workout_df.iterrows():\n",
    "        mask = (timeline.index >= workout['start_time']) & (timeline.index <= workout['end_time'])\n",
    "        timeline.loc[mask, 'Workout'] = 1\n",
    "    \n",
    "    # Mark nutrition events (±30 minutes window)\n",
    "    for meal_time in nutrition_df.index:\n",
    "        start_window = meal_time - pd.Timedelta('30T')\n",
    "        end_window = meal_time + pd.Timedelta('30T')\n",
    "        mask = (timeline.index >= start_window) & (timeline.index <= end_window)\n",
    "        timeline.loc[mask, 'Nutrition'] = 1\n",
    "    \n",
    "    return timeline\n",
    "\n",
    "# Create integrated timeline\n",
    "activity_timeline = create_activity_timeline(glucose_df, sleep_df, workout_df, nutrition_df)\n",
    "\n",
    "print(f\"📊 Activity Timeline Created\")\n",
    "print(f\"   Total time points: {len(activity_timeline):,}\")\n",
    "print(f\"   Sleep periods marked: {(activity_timeline['Sleep'] == 1).sum():,} points\")\n",
    "print(f\"   Workout periods marked: {(activity_timeline['Workout'] == 1).sum():,} points\")\n",
    "print(f\"   Nutrition periods marked: {(activity_timeline['Nutrition'] == 1).sum():,} points\")"
