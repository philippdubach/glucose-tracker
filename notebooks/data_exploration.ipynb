    "    \n",
    "except FileNotFoundError as e:\n",
    "    print(f\"❌ Data files not found: {e}\")\n",
    "    print(\"💡 Run the quickstart.py script first to generate sample data\")\n",
    "    print(\"   python quickstart.py --create-sample\")"
